# Evaluation Configuration# Evaluation Configuration

# Evaluate model on test sets from raw audio# Architecture: WavLM + Attentive Pooling + LayerNorm + Deeper Heads

# Copy this file to eval.yaml and update paths# Copy this file to eval.yaml and update paths



# Model# Model

model:model:

  checkpoint: "path/to/best_model"  checkpoint: "path/to/best_model"

  name: "microsoft/wavlm-base-plus"  name: "microsoft/wavlm-base-plus"

  head_hidden_dim: 256  head_hidden_dim: 256



# Audio Processing# Audio Processing

audio:audio:

  sampling_rate: 16000  sampling_rate: 16000

  max_duration: 5  max_duration: 5



# Evaluation# Evaluation

evaluation:evaluation:

  batch_size: 32  batch_size: 32

  dataloader_num_workers: 2  dataloader_num_workers: 2



# Data Paths# Data Paths (UPDATE THESE PATHS)

data:data:

  clean_test_meta: "path/to/metadata/clean_testset.csv"  clean_test_meta: "path/to/metadata/clean_testset.csv"

  clean_test_audio: "path/to/clean_testset"  clean_test_audio: "path/to/clean_testset"

  noisy_test_meta: "path/to/metadata/noisy_testset.csv"  noisy_test_meta: "path/to/metadata/noisy_testset.csv"

  noisy_test_audio: "path/to/noisy_testset"  noisy_test_audio: "path/to/noisy_testset"



# Output# Output

output:output:

  dir: "output/evaluation"  dir: "output/evaluation"

  save_predictions: true  save_predictions: true

  save_confusion_matrix: true  save_confusion_matrix: true



